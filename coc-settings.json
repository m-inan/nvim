{
  "suggest.noselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "dart",
    "json",
    "graphql",
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "languageserver": {
    "dart": {
      "command": "dart",
      "args": [
        "/usr/local/opt/dart/libexec/bin/snapshots/analysis_server.dart.snapshot",
        "--lsp",
        "--client-id",
        "vim",
        "--client-version",
        "coc.nvim"
      ],
      "filetypes": ["dart"],
      "trace.server": "verbose"
    }
  }
}
